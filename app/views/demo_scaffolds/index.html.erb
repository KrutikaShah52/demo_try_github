<p id="notice"><%= notice %></p>

<h1>Demo Scaffolds</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @demo_scaffolds.each do |demo_scaffold| %>
      <tr>
        <td><%= demo_scaffold.name %></td>
        <td><%= link_to 'Show', demo_scaffold %></td>
        <td><%= link_to 'Edit', edit_demo_scaffold_path(demo_scaffold) %></td>
        <td><%= link_to 'Destroy', demo_scaffold, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Demo Scaffold', new_demo_scaffold_path %>
